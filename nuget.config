<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<!-- remove any machine-wide sources with
		<clear/> -->
		<clear/>
		<!-- add an Azure Artifacts feed -->
		<add key="nuget-inps"
		     value="http://nexus.inps.it/repository/nuget-group/index.json"/>
	</packageSources>
	<packageSourceCredentials>
		<nuget-inps>
			<add key="Username"
			     value="#{username}#"/>
			<add key="ClearTextPassword"
			     value="#{password}#"/>
		</nuget-inps>
	</packageSourceCredentials>
</configuration>